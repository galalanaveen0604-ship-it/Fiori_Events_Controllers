<mvc:View controllerName="org.index.it.sapfiori.controller.Events_Controller"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page id="page" title="{i18n>screenTitle}">
        <content>
            <Input id="_idGenInput" description="Any Value" placeholder="{i18n>placeholderText}" type="Number"/>
            <Input id="_idGenInput1" description="Any Value" placeholder="{i18n>placeholderText}" type="Number"/>
            <Button id="idGenButton1" text="Plus" width="150px" icon="sap-icon://add" press="onClick"/>
            <Button id="idGenButton2" text="Minus" width="150px" icon="sap-icon://less" press="onClick"/>
            <Button id="idGenButton3" text="Multiplication" width="150px" icon="sap-icon://trend-up" press="onClick"/>
            <Button id="idGenButton4" text="Division" width="150px" icon="sap-icon://trend-down" press="onClick"/>
        </content>
    </Page>
</mvc:View>